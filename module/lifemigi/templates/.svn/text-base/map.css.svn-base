.map_base { position: relative; margin: 0 auto; }
.div { position: absolute; display: inline; }
.div a { position: absolute; display: block; }
.div a span { display: none; width: 400px; border: 1px solid black; color: black; text-align: left; background: #ddd; position: relative; left: 40px; top: 40px; }
.map_base .div a:hover { margin: 0; }
.div a:hover span { display: block; }

